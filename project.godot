[application]

config/name="Digimon TCG"
run/main_scene="res://Assets/Scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
OnlineMatch="*res://addons/nakama-webrtc/OnlineMatch.gd"
Online="*res://Assets/Autoload/Online.gd"
GameState="*res://Assets/Autoload/GameState.gd"
Build="*res://Assets/Autoload/Build.gd"
DefaultDeck="*res://Assets/Autoload/default_deck.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=800
window/size/test_height=600
window/stretch/mode="2d"

[gdnative]

singletons=[ "res://webrtc/webrtc.gdnlib", "res://webrtc_debug/webrtc_debug.gdnlib" ]

[input]

click={
